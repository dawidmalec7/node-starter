{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User",
  "description": "The User",
  "required": ["email", "password", "first_name", "last_name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The user email. Must be unique.",
      "examples": ["test@test.pl"]
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The user hashed password"
    },
    "first_name": {
      "$id": "#/properties/first_name",
      "type": "string",
      "title": "The user first_name",
      "examples": ["Dawid"]
    },
    "last_name:": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "The user last_name",
      "examples": ["Elo"]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The user created_at"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The user updated_at"
    }
  }
}
